{
  "presets": ["env", "react", "stage-0"],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    }
  },
  "plugins": [
    "add-module-exports",
    "transform-es2015-modules-commonjs",
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "client": "./client",
        "modules": "./client/App/modules",
      }
    }]
  ]
}
